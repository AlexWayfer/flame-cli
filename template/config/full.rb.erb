# frozen_string_literal: true

## Config split into `base` and `full` because of `prune_bundler`:
## https://github.com/puma/puma/issues/2319
module <%= @module_name %>
	class << self
		using GorillaPatch::Inflections

		def complete_config(config)
			%w[Sentry R18n Mail Sequel Shrine].each do |processor_name|
				require_relative "processors/#{processor_name.underscore}"
				<%= @short_module_name %>::Config::Processors.const_get(processor_name).new config
			end
		end
	end
end
